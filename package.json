  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-pack": "npm run build && electron-builder",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --icon=D:/Karan/React Tasks/user-management-app/public/favicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"USER-MANAGEMENT-APP\"",
    "react-build": "react-scripts build",
    "electron-dev": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "npm run react-build && electron-builder",
    "package": "npm run build && electron-packager . UserManagementApp --overwrite --asar --platform=win32 --arch=x64 --out=release-builds --icon=assets/icon.ico --prune=true --ignore=\"node_modules/fsevents\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "concurrently": "^9.2.1",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
