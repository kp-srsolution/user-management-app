    "wait-on": "^8.0.4"
  },
  "build": {
    "appId": "com.example.my-electron-react-app",
    "productName": "User Management App",
    "files": [
      "!node_modules/fsevents",
      "build/**/*",
      "public/electron.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    }
  }
